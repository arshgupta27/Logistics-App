  <!-- ======= Hero Section ======= -->
  <section id="hero" class="hero d-flex align-items-center">
    <div class="container">
      <div class="row gy-4 d-flex justify-content-between">
        <div class="col-lg-6 order-2 order-lg-1 d-flex flex-column justify-content-center">
          <h2 data-aos="fade-up">Your Delivery, Delivered Right</h2>
          <p data-aos="fade-up" data-aos-delay="100">
            We aim to provide customers with fast, reliable, and
            cost-effective delivery services. We also strive to ensure that
            our customers have access to accurate tracking information and
            customer service representatives who are available to answer your
            question
          </p>

          <form action="/trackOrder" method="post" class="form-search d-flex align-items-stretch mb-3"
            onsubmit="return validation();">
            <input type="text" class="form-control" name="packageID" placeholder="Parcel Tracking Number" required />
            <button type="submit" class="btn btn-primary-setup">
              Track
            </button>
          </form>

          <script>
            function validation() {
              var pathLength = Number("<%=packg.path.length%>");
              var cities = "<%=packg.path%>".toString();
              var citiesArray = cities.split(",");
              var index = 0;
              var interVal = setInterval(function () {
                document.getElementById("current-city").innerHTML = citiesArray[index++];
                document.getElementById("current_city_line").className += " completed";
                document.getElementById("cities_left").innerHTML = `Your package has yet to travel ${citiesArray.length - index} cities!`
                if (citiesArray[index] == citiesArray[citiesArray.length - 1]) {
                  document.getElementById("cities_left").innerHTML = `Package Arrived!`
                  document.getElementById("destination_line").className += " completed";
                  clearInterval(interVal);
                }
              }, 4000);

              $('#popup-track-order').modal('show');
            }
          </script>

          <!-- <div></div> -->
          <div class="row gy-4" data-aos="fade-up" data-aos-delay="400">
            <div class="col-lg-4 col-6">
