  <main id="main">
    <!-- ------------ORDER TRACKING POP UP------------- -->
    <div id="popup-track-order" class="modal fade modal-lg ">
      <div class="modal-dialog modal-dialog-centered bg-transparent">
        <div class="modal-content trackingCard">
          <div class="modal-body ">
            <!-- <span class="text-btn" data-bs-dismiss="modal"
                >Go back
              </span> -->
            <div class="container px-md-4 py-5 mx-auto px-2 ">
              <div class="innerCard ">

                <div class="row d-flex justify-content-between statusBar">

                  <div class="d-flex justify-content-between">
                    <h3>
                      ORDER
                    </h3>


                    <div class="flex-column text-sm-right ">

                      <p style="font-size: 25px">
                        <span class="font-weight-bold" style="color:  #c51f1f;">
                          <%=packg.packageID%>
                        </span>
                      </p>
                    </div>
                  </div>


                </div>
                <div class="container">

                  <div class="row">
                    <div class="col-12 col-md-10 hh-grayBox pt45 pb20">
                      <div class="flex-column text-sm-center ">

                        <p style="font-size: 25px">
                          <span id="cities_left" class="font-weight-bold" style="color: #fff;">Your package has yet to
                            travel <%=packg.path.length%> cities!</span>
                        </p>
                      </div>
                      <div class="row justify-content-between">
                        <!-- Order Shipped -->
                        <div class="order-tracking completed">
                          <span class="is-complete"></span>
                          <div class="iconBox">
                            <i class="bi bi-truck"></i>
                            <div class="d-flex flex-column">
                              <p class="font-weight-bold">Order Shipped</p>
                              <h5 style="color: var(--color-primary); margin-top: 10px;">
                                <%=packg.sourceCity%>
                              </h5>
                            </div>
                          </div>
                        </div>
                        <!-- en Route -->
                        <div id="current_city_line" class="order-tracking">
                          <span class="is-complete"></span>
                          <div class="iconBox">
                            <i class="bi bi-truck-front"></i>
                            <div class="d-flex flex-column">
                              <p class="font-weight-bold">Order En Route</p>
                              <h5 id="current-city" style="color: var(--color-primary); margin-top: 10px;"></h5>
                            </div>
                          </div>
                        </div>
                        <!-- Order Delivered -->
                        <div id="destination_line" class="order-tracking">
                          <span class="is-complete"></span>
                          <div class="iconBox">
                            <i class="bi bi-box-seam-fill"></i>
                            <div class="d-flex flex-column">
                              <p class="font-weight-bold">Order Arrived</p>
                              <h5 style="color: var(--color-primary); margin-top: 10px;">
                                <%=packg.destinationCity%>
                              </h5>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

            </div>
          </div>
        </div>
